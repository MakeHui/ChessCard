{
    "extends": "eslint-config-airbnb-es5",
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "indent": ["error", 4],
        "func-names": "off",
        "new-cap": "off",
        "valid-jsdoc": "off",
        "comma-dangle": "off",
        "eol-last": "off",
        "no-param-reassign": "off",
        "no-undefined": "off",
        "eqeqeq": "off",
        "brace-style": "off",
        "space-before-function-paren": "off"
    },
    "globals": {
        "window": true,
        "Math": true,
        "WebSocket": true,
        "Global": true,
        "WebSocketManager": true,
        "HttpRequestManager": true,
        "NativeExtensionManager": true,
        "Tools": true,
        "Animation": true,
        "Dialog": true,
        "proto": true,
        "goog": true,
        "ReconnectingWebSocket": true,
        "clone": true,
        "md5": true,
        "cc": true,
        "gl": true,
        "CC_JSB": true,
        "jsb": true
    }
}